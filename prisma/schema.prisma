generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Alien {
  id              Int     @id @default(autoincrement())
  name            String
  ultimateForm    Boolean
  species         String
  homeWorld       String
  ability         String
  imgUrl          String
  firstAppearance String
}
